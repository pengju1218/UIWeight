<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="SizeRadioStyle">
        <attr name="sizeRadio_drawablePadding" format="dimension" />
        <attr name="sizeRadio_drawableSize" format="dimension" />
        <attr name="sizeRadio_drawableTop" format="reference" />
        <attr name="sizeRadio_drawableLeft" format="reference" />
        <attr name="sizeRadio_drawableRight" format="reference" />
        <attr name="sizeRadio_drawableBottom" format="reference" />
    </declare-styleable>

    <declare-styleable name="PasswordInputView">
        <attr name="pivBorderColor" format="color" />
        <attr name="pivBorderWidth" format="dimension" />
        <attr name="pivBorderRadius" format="dimension" />
        <attr name="pivPasswordColor" format="color" />
        <attr name="pivPasswordWidth" format="dimension" />
        <attr name="pivPasswordRadius" format="dimension" />
        <attr name="pivPasswordLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="TabLineStyle">

        <attr name="tab_LineColor" format="color" />
        <attr name="tab_TextSize" format="dimension" />
        <attr name="tabItem_Width" format="dimension" />
        <attr name="tab_BgColor" format="color" />
        <attr name="tab_SelectColor" format="color" />
        <attr name="tab_UnSelectColor" format="color" />
        <attr name="tab_divide_Visiable" format="boolean" />
    </declare-styleable>


    <declare-styleable name="KeyTextStyle">
        <attr name="key_name_Color" format="color" />
        <attr name="key_name_Size" format="dimension" />
        <attr name="key_value_Color" format="color" />
        <attr name="key_value_Size" format="dimension" />
        <attr name="key_name" format="string" />
        <attr name="key_value" format="string" />
    </declare-styleable>

    <declare-styleable name="PasswordView">
        <attr name="pass2_borderWidth" format="dimension" />
        <attr name="pass2_borderColor" format="color" />
        <attr name="pass2_borderRadius" format="dimension" />
        <attr name="pass2_passwordLength" format="integer" />
        <attr name="pass2_passwordWidth" format="dimension" />
        <attr name="pass2_passwordColor" format="color" />
        <attr name="pass2_passwordRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="dashedline">
        <attr name="dash_lineColor" format="color" />
        <attr name="dash_vertical" format="boolean" />
        <attr name="dash_lineWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DashView">
        <attr name="dashWidth" format="dimension" />
        <attr name="dashlineWidth" format="dimension" />
        <attr name="dashlineHeight" format="dimension" />
        <attr name="dashlineColor" format="color" />
        <attr name="dashOrientation" format="integer" />

    </declare-styleable>


    <declare-styleable name="SlingTab">
        <attr name="Slingtab_textColor" format="color" />
        <attr name="scroller" format="boolean" />
        <attr name="textDefColor" format="color" />
        <attr name="textFocusColor" format="color" />
        <attr name="textPadding" format="dimension" />
        <attr name="Slingtab_textSize" format="dimension" />
        <attr name="slinglineHeight" format="dimension" />
        <attr name="slinglineColor" format="color" />
        <attr name="line_marginTop" format="dimension" />
    </declare-styleable>


    <attr name="cropImageStyle" format="reference" />

    <declare-styleable name="CropImageView">
        <attr name="highlightColor" format="reference|color" />
        <attr name="showThirds" format="boolean" />
        <attr name="showHandles">
            <enum name="changing" value="0" />
            <enum name="always" value="1" />
            <enum name="never" value="2" />
        </attr>
    </declare-styleable>




    <declare-styleable name="TypeScroller">
        <attr name="typescroller_Vspace" format="dimension"></attr>
        <attr name="typescroller_Hspace" format="dimension"></attr>
        <attr name="typescroller_first_left" format="dimension"></attr>
    </declare-styleable>


    <declare-styleable name="NiceSpinner">
        <attr name="arrowTint" format="color" />
        <attr name="hideArrow" format="boolean" />
        <attr name="backgroundSelector" format="integer"/>
        <attr name="textTint" format="color"/>
    </declare-styleable>


  <!--  <declare-styleable name="ShowPercentView">
        <attr name="percent" format="integer"></attr>
        <attr name="allLineColor" format="color" />
        <attr name="percentLineColorLow" format="color" />
        <attr name="percentLineColorHight" format="color" />
    </declare-styleable>
-->


    <declare-styleable name="edit_line">
        <attr name="edit_lineColor" format="color" />
    </declare-styleable>


    <declare-styleable name="imagetext">
        <attr name="imgT_text" format="string" />
        <attr name="imgT_img" format="reference" />
        <attr name="imgT_top" format="dimension"></attr>

        <attr name="imgT_size" format="dimension"></attr>
        <attr name="imgT_height" format="dimension"></attr>
        <attr name="imgT_width" format="dimension"></attr>
        <attr name="imgT_color" format="color"></attr>

    </declare-styleable>


    <declare-styleable name="tabViewAlone">
        <attr name="tabViewAlone_UpDraw" format="reference"/>
        <attr name="tabViewAlone_iconPadding" format="dimension"/>
        <attr name="tabViewAlone_iconWidth" format="dimension"/>
        <attr name="tabViewAlone_iconHeight" format="dimension"/>
        <attr name="tabViewAlone_tab_textSize" format="dimension"/>
    </declare-styleable>


</resources>